<!-- Modal -->
<div id="graph-modal" class="modal" tabindex="-1" role="dialog">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
            {{#if user}}
        <h5 class="modal-title">Create Graph Pin</h5>
            {{else}}
        <h5 class="modal-title">Sorry...</h5>
            {{/if}}
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <p id="graph-modal-text">You will need an account to save your custom graph data!</p>
      </div>
      <div class="modal-footer">
        {{#if user}}<button id="submit-modal" type="button" class="btn btn-primary">Save changes</button>{{/if}}
        <button id="close-modal" type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
